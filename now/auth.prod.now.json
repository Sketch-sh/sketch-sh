{
  "name": "rtop-auth",
  "public": true,
  "type": "docker",
  "alias": "auth.sketch.sh",
  "env": {
    "NODE_ENV": "production",
    "JWT_TOKEN": "@sketch-jwt-token",
    "HASURA_ENDPOINT": "https://sketch-graphql.herokuapp.com/v1alpha1/graphql",
    "GITHUB_CLIENT_ID": "@sketch-gh-id",
    "GITHUB_CLIENT_SECRET": "@sketch-gh-secret",
    "FRONTEND_URL": "https://sketch.sh"
  }
}
